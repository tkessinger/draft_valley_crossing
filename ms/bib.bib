	Month = dec,
	Number = {1},
	Pages = {143},
	Shorttitle = {With a little help from my friends},
	Title = {With a little help from my friends: cooperation can accelerate the rate of adaptive valley crossing},
	Url = {https://link.springer.com/article/10.1186/s12862-017-0983-2},
	Volume = {17},
	Year = {2017},
