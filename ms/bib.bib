	Issn = {0022-5193},
	Journal = {J Theor Biol},
	Keywords = {Adaptive valley crossing, Fitness landscape, Moment closure, Population genetics, Spatial structure},
	Month = sep,
	Pages = {123--133},
	Shorttitle = {Tipping the mutation–selection balance},
	Title = {Tipping the mutation–selection balance: Limited migration increases the frequency of deleterious mutants},
	Url = {http://www.sciencedirect.com/science/article/pii/S0022519315002313},
