	Keywords = {Evolutionary genetics, Molecular evolution, Natural selection, Fungal evolution, Gene duplication, Green fluorescent protein, Saccharomyces cerevisiae, Sequence motif analysis},
	Language = {en},
	Month = apr,
	Number = {4},
	Pages = {e1006735},
	Title = {Parallel reorganization of protein function in the spindle checkpoint pathway through evolutionary paths in the fitness landscape that appear neutral in laboratory experiments},
	Url = {http://journals.plos.org/plosgenetics/article?id=10.1371/journal.pgen.1006735},
