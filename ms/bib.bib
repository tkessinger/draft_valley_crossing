	Language = {en},
	Month = sep,
	Number = {1742},
	Pages = {3418--3425},
	Pmid = {22719036},
	Title = {Environmental change exposes beneficial epistatic interactions in a catalytic RNA},
	Url = {http://rspb.royalsocietypublishing.org.proxy.lib.duke.edu/content/279/1742/3418},
