	Issn = {1558-5646},
	Journal = {Evolution},
	Keywords = {Deleterious fixations, Fisher's geometric model, house of cards, reversible Markov chain, weak mutation},
	Language = {en},
	Month = may,
	Num-Scopus-Cites = {0},
	Number = {5},
	Pages = {1351--1364},
	Title = {The Inevitability of Unconditionally Deleterious Substitutions During Adaptation},
	Url = {http://onlinelibrary.wiley.com/doi/10.1111/evo.12350/abstract},
